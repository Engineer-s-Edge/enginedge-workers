{
  "dashboard": {
    "id": null,
    "title": "Scheduling Worker - Production Dashboard",
    "tags": ["scheduling", "production", "monitoring"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Service Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"scheduling-worker\"}",
            "legendFormat": "Service Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Connections",
        "type": "gauge",
        "targets": [
          {
            "expr": "mongodb_connections{state=\"active\", db=\"scheduling_prod\"}",
            "legendFormat": "Active Connections"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 70
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "Request Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 4,
        "title": "Response Time (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 5,
        "title": "Error Rate by Status",
        "type": "bargauge",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"4..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "4xx Errors"
          },
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "5xx Errors"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 6,
        "title": "Database Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mongodb_op_counters_total{type=\"query\"}[5m])",
            "legendFormat": "Queries/sec"
          },
          {
            "expr": "rate(mongodb_op_counters_total{type=\"insert\"}[5m])",
            "legendFormat": "Inserts/sec"
          },
          {
            "expr": "rate(mongodb_op_counters_total{type=\"update\"}[5m])",
            "legendFormat": "Updates/sec"
          },
          {
            "expr": "rate(mongodb_op_counters_total{type=\"delete\"}[5m])",
            "legendFormat": "Deletes/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 7,
        "title": "Cache Performance",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m])) * 100",
            "legendFormat": "Cache Hit Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 18
        }
      },
      {
        "id": 8,
        "title": "Memory Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"scheduling-worker\"} / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB",
            "min": 0,
            "max": 1024,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 700
                },
                {
                  "color": "red",
                  "value": 900
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 18
        }
      },
      {
        "id": 9,
        "title": "Kafka Consumer Lag",
        "type": "graph",
        "targets": [
          {
            "expr": "kafka_consumer_group_lag{group=\"scheduling-worker-group\"}",
            "legendFormat": "Consumer Lag - {{topic}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        }
      },
      {
        "id": 10,
        "title": "Calendar Sync Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(calendar_sync_operations_total[5m])",
            "legendFormat": "Sync Operations/sec"
          },
          {
            "expr": "rate(calendar_sync_errors_total[5m])",
            "legendFormat": "Sync Errors/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        }
      },
      {
        "id": 11,
        "title": "Goal & Habit Operations",
        "type": "bargauge",
        "targets": [
          {
            "expr": "rate(goal_operations_total[5m])",
            "legendFormat": "Goal Operations"
          },
          {
            "expr": "rate(habit_operations_total[5m])",
            "legendFormat": "Habit Operations"
          },
          {
            "expr": "rate(habit_completion_total[5m])",
            "legendFormat": "Habit Completions"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 30
        }
      },
      {
        "id": 12,
        "title": "ML Scheduling Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ml_scheduling_requests_total[5m])",
            "legendFormat": "ML Requests/sec"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ml_scheduling_duration_seconds_bucket[5m]))",
            "legendFormat": "ML Processing Time P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 30
        }
      },
      {
        "id": 13,
        "title": "System Resources",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 38
        }
      },
      {
        "id": 14,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_user_seconds_total{job=\"scheduling-worker\"}[5m]) * 100",
            "legendFormat": "CPU Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 39
        }
      },
      {
        "id": 15,
        "title": "Disk I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_io_read_bytes_total{job=\"scheduling-worker\"}[5m]) / 1024 / 1024",
            "legendFormat": "Read MB/s"
          },
          {
            "expr": "rate(process_io_write_bytes_total{job=\"scheduling-worker\"}[5m]) / 1024 / 1024",
            "legendFormat": "Write MB/s"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB/s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 39
        }
      },
      {
        "id": 16,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_io_read_bytes_total{job=\"scheduling-worker\"}[5m]) / 1024 / 1024",
            "legendFormat": "Network In MB/s"
          },
          {
            "expr": "rate(process_io_write_bytes_total{job=\"scheduling-worker\"}[5m]) / 1024 / 1024",
            "legendFormat": "Network Out MB/s"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB/s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 39
        }
      },
      {
        "id": 17,
        "title": "Application Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"scheduling-worker\"}",
            "legendFormat": ""
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 47
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}