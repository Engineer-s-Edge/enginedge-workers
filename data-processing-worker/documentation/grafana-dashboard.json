          }
        }
      },
      {
        "id": 13,
        "title": "Total Requests (5m rate)",
        "type": "stat",
        "gridPos": {
          "x": 18,
          "y": 40,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(rate(data_processing_requests_total[5m]))",
