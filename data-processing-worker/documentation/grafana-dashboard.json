{
  "dashboard": {
    "title": "Phase 8 - RAG Pipeline & Cross-Worker Communication",
    "tags": ["phase8", "rag", "cross-worker", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "RAG Search Latency (p95 & p99)",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(rag_search_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(rag_search_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "show": true
          }
        ],
        "alert": {
          "name": "High RAG Search Latency",
          "conditions": [
            {
              "evaluator": {
                "params": [2],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "RAG search p95 latency exceeds 2 seconds",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 2,
        "title": "RAG Search Throughput",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(rag_search_total[5m])",
            "legendFormat": "Searches/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Requests per second",
            "show": true
          }
        ]
      },
      {
        "id": 3,
        "title": "Document Processing Latency",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(data_processing_request_latency_seconds_bucket{endpoint=\"/documents/process-for-rag\"}[5m]))",
            "legendFormat": "p95 Process Document",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(data_processing_request_latency_seconds_bucket{endpoint=\"/documents/process-for-rag\"}[5m]))",
            "legendFormat": "p99 Process Document",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency",
            "show": true
          }
        ],
        "alert": {
          "name": "High Document Processing Latency",
          "conditions": [
            {
              "evaluator": {
                "params": [3],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "Document processing p95 latency exceeds 3 seconds",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 4,
        "title": "DLQ Message Rate by Topic",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(dlq_messages_total[5m])",
            "legendFormat": "{{topic}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Messages/sec",
            "show": true
          }
        ],
        "alert": {
          "name": "High DLQ Message Rate",
          "conditions": [
            {
              "evaluator": {
                "params": [10],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "DLQ message rate exceeds 10/sec - investigate failures",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 5,
        "title": "Kafka Message Processing Time",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(kafka_message_processing_time_seconds_bucket[5m]))",
            "legendFormat": "p95 {{topic}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(kafka_message_processing_time_seconds_bucket[5m]))",
            "legendFormat": "p99 {{topic}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Processing Time",
            "show": true
          }
        ]
      },
      {
        "id": 6,
        "title": "Kafka Consumer Lag by Topic",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "kafka_lag",
            "legendFormat": "{{topic}} - partition {{partition}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Messages Behind",
            "show": true
          }
        ],
        "alert": {
          "name": "High Kafka Consumer Lag",
          "conditions": [
            {
              "evaluator": {
                "params": [1000],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "Kafka consumer lag exceeds 1000 messages",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 7,
        "title": "Data Processing Worker Error Rate",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(data_processing_errors_total[5m])",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Errors/sec",
            "show": true
          }
        ],
        "alert": {
          "name": "High Error Rate",
          "conditions": [
            {
              "evaluator": {
                "params": [5],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "frequency": "1m",
          "handler": 1,
          "message": "Error rate exceeds 5/sec in Data Processing Worker",
          "noDataState": "no_data",
          "executionErrorState": "alerting"
        }
      },
      {
        "id": 8,
        "title": "RAG Search Results Count",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(rag_search_results_total[5m])",
            "legendFormat": "Results returned/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Results per second",
            "show": true
          }
        ]
      },
      {
        "id": 9,
        "title": "Kafka Message Throughput by Topic",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 32,
          "w": 24,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(kafka_messages_total[5m])",
            "legendFormat": "{{topic}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Messages/sec",
            "show": true
          }
        ]
      },
      {
        "id": 10,
        "title": "Service Health Status",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 40,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "up{job=\"assistant-worker\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "value": 0,
                  "color": "red"
                },
                {
                  "value": 1,
                  "color": "green"
                }
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {}
        }
      },
      {
        "id": 11,
