{
  "dashboard": {
    "id": null,
    "title": "Assistant Worker - Production Dashboard",
    "tags": ["assistant", "ai", "agents", "memory", "knowledge-graph", "production", "monitoring"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Service Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"assistant-worker\"}",
            "legendFormat": "Service Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Agents",
        "type": "gauge",
        "targets": [
          {
            "expr": "agents_active_total{job=\"assistant-worker\"}",
            "legendFormat": "Active Agents"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 75
                },
                {
                  "color": "red",
                  "value": 90
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Agent Execution Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(agent_executions_total{job=\"assistant-worker\"}[5m])",
            "legendFormat": "Executions/sec"
          },
          {
            "expr": "rate(agent_executions_success_total{job=\"assistant-worker\"}[5m])",
            "legendFormat": "Success/sec"
          },
          {
            "expr": "rate(agent_executions_error_total{job=\"assistant-worker\"}[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 4,
        "title": "Agent Execution Duration (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_execution_duration_seconds_bucket{job=\"assistant-worker\"}[5m]))",
            "legendFormat": "P95 Duration"
          },
          {
            "expr": "histogram_quantile(0.50, rate(agent_execution_duration_seconds_bucket{job=\"assistant-worker\"}[5m]))",
            "legendFormat": "P50 Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 5,
        "title": "Agent Success Rate by Type",
        "type": "bargauge",
        "targets": [
          {
            "expr": "rate(agent_executions_success_total{job=\"assistant-worker\"}[5m]) / rate(agent_executions_total{job=\"assistant-worker\"}[5m]) * 100",
            "legendFormat": "{{agent_type}} Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 90
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 6,
        "title": "Memory Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(memory_operations_total{operation=\"store\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "Memory Stores"
          },
          {
            "expr": "rate(memory_operations_total{operation=\"retrieve\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "Memory Retrieves"
          },
          {
            "expr": "rate(memory_operations_total{operation=\"clear\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "Memory Clears"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 7,
        "title": "Knowledge Graph Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(kg_operations_total{operation=\"query\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "KG Queries"
          },
          {
            "expr": "rate(kg_operations_total{operation=\"insert\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "KG Inserts"
          },
          {
            "expr": "rate(kg_operations_total{operation=\"update\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "KG Updates"
          },
          {
            "expr": "rate(kg_operations_total{operation=\"delete\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "KG Deletes"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 8,
        "title": "LLM API Calls",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(llm_api_calls_total{job=\"assistant-worker\"}[5m])",
            "legendFormat": "LLM Calls/sec"
          },
          {
            "expr": "histogram_quantile(0.95, rate(llm_api_duration_seconds_bucket{job=\"assistant-worker\"}[5m]))",
            "legendFormat": "LLM Call P95 Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 9,
        "title": "Agent Type Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (agent_type) (rate(agent_executions_total{job=\"assistant-worker\"}[5m]))",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 10,
        "title": "Conversation Sessions",
        "type": "stat",
        "targets": [
          {
            "expr": "conversations_active_total{job=\"assistant-worker\"}",
            "legendFormat": "Active Conversations"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 200
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 8,
          "x": 8,
          "y": 28
        }
      },
      {
        "id": 11,
        "title": "Token Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(llm_tokens_total{type=\"input\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "Input Tokens/sec"
          },
          {
            "expr": "rate(llm_tokens_total{type=\"output\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "Output Tokens/sec"
          },
          {
            "expr": "rate(llm_tokens_total{type=\"total\", job=\"assistant-worker\"}[5m])",
            "legendFormat": "Total Tokens/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 28
        }
      },
      {
        "id": 12,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"assistant-worker\"}[5m])",
            "legendFormat": "Request Rate"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 36
        }
      },
      {
        "id": 13,
        "title": "HTTP Response Time (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"assistant-worker\"}[5m]))",
            "legendFormat": "P95 Response Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 36
        }
      },
      {
        "id": 14,
        "title": "System Resources",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 44
        }
      },
      {
        "id": 15,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_user_seconds_total{job=\"assistant-worker\"}[5m]) * 100",
            "legendFormat": "CPU Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 45
        }
      },
      {
        "id": 16,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"assistant-worker\"} / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 45
        }
      },
      {
        "id": 17,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_io_read_bytes_total{job=\"assistant-worker\"}[5m]) / 1024 / 1024",
            "legendFormat": "Network In MB/s"
          },
          {
            "expr": "rate(process_io_write_bytes_total{job=\"assistant-worker\"}[5m]) / 1024 / 1024",
            "legendFormat": "Network Out MB/s"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB/s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 45
        }
      },
      {
        "id": 18,
        "title": "Application Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{job=\"assistant-worker\"}",
            "legendFormat": ""
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": true,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 53
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}