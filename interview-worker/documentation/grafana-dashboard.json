{
  "dashboard": {
    "id": null,
    "title": "Interview Worker - Production Dashboard",
    "tags": ["interview", "ai", "production", "monitoring"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Service Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"interview-worker\"}",
            "legendFormat": "Service Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Interview Sessions",
        "type": "gauge",
        "targets": [
          {
            "expr": "interview_sessions_active{job=\"interview-worker\"}",
            "legendFormat": "Active Sessions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "orange",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Session Status Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status) (interview_sessions_total{job=\"interview-worker\"})",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Sessions Created Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(interview_sessions_created_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Created/sec"
          },
          {
            "expr": "rate(interview_sessions_completed_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Completed/sec"
          },
          {
            "expr": "rate(interview_sessions_paused_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Paused/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        }
      },
      {
        "id": 5,
        "title": "Question Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(interview_questions_selected_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Selected/sec"
          },
          {
            "expr": "rate(interview_responses_submitted_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Responses/sec"
          },
          {
            "expr": "rate(interview_questions_skipped_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Skipped/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 6,
        "title": "Profile Observations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(candidate_profile_observations_total{type=\"strengths\",job=\"interview-worker\"}[5m])",
            "legendFormat": "Strengths/sec"
          },
          {
            "expr": "rate(candidate_profile_observations_total{type=\"concerns\",job=\"interview-worker\"}[5m])",
            "legendFormat": "Concerns/sec"
          },
          {
            "expr": "rate(candidate_profile_observations_total{type=\"keyInsights\",job=\"interview-worker\"}[5m])",
            "legendFormat": "Insights/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 7,
        "title": "WebSocket Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "websocket_connections_active{job=\"interview-worker\"}",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 8,
        "title": "WebSocket Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(websocket_messages_received_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Messages Received/sec"
          },
          {
            "expr": "rate(websocket_messages_sent_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Messages Sent/sec"
          },
          {
            "expr": "rate(websocket_connections_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Connections/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 12
        }
      },
      {
        "id": 9,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Requests/sec"
          },
          {
            "expr": "rate(http_requests_total{method=\"POST\",job=\"interview-worker\"}[5m])",
            "legendFormat": "POST/sec"
          },
          {
            "expr": "rate(http_requests_total{method=\"GET\",job=\"interview-worker\"}[5m])",
            "legendFormat": "GET/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 10,
        "title": "HTTP Response Time (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"interview-worker\"}[5m]))",
            "legendFormat": "P95 Response Time"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"interview-worker\"}[5m]))",
            "legendFormat": "P50 Response Time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 20
        }
      },
      {
        "id": 11,
        "title": "HTTP Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\",job=\"interview-worker\"}[5m])",
            "legendFormat": "5xx Errors/sec"
          },
          {
            "expr": "rate(http_requests_total{status=~\"4..\",job=\"interview-worker\"}[5m])",
            "legendFormat": "4xx Errors/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 12,
        "title": "MongoDB Connection Pool",
        "type": "stat",
        "targets": [
          {
            "expr": "mongodb_connection_pool_size{job=\"interview-worker\"}",
            "legendFormat": "Pool Size"
          },
          {
            "expr": "mongodb_connection_pool_active{job=\"interview-worker\"}",
            "legendFormat": "Active Connections"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 28
        }
      },
      {
        "id": 13,
        "title": "MongoDB Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mongodb_operations_total{operation=\"find\",job=\"interview-worker\"}[5m])",
            "legendFormat": "Find Ops/sec"
          },
          {
            "expr": "rate(mongodb_operations_total{operation=\"insert\",job=\"interview-worker\"}[5m])",
            "legendFormat": "Insert Ops/sec"
          },
          {
            "expr": "rate(mongodb_operations_total{operation=\"update\",job=\"interview-worker\"}[5m])",
            "legendFormat": "Update Ops/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 28
        }
      },
      {
        "id": 14,
        "title": "System Resources",
        "type": "row",
        "gridPos": {
          "h": 1,
          "w": 24,
          "x": 0,
          "y": 36
        }
      },
      {
        "id": 15,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_user_seconds_total{job=\"interview-worker\"}[5m]) * 100",
            "legendFormat": "CPU Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 0,
          "y": 37
        }
      },
      {
        "id": 16,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"interview-worker\"} / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "MB"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 37
        }
      },
      {
        "id": 17,
        "title": "Report Generation",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(interview_reports_generated_total{job=\"interview-worker\"}[5m])",
            "legendFormat": "Reports/sec"
          },
          {
            "expr": "histogram_quantile(0.95, rate(interview_report_generation_duration_seconds_bucket{job=\"interview-worker\"}[5m]))",
            "legendFormat": "P95 Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 37
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}
