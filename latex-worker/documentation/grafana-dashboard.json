{
  "dashboard": {
    "title": "LaTeX Worker - Production Dashboard",
    "tags": ["latex", "worker", "enginedge"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Compilation Success Rate",
        "type": "stat",
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "rate(latex_compilation_success_total[5m]) / rate(latex_compilation_total[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 80, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Queue Depth",
        "type": "gauge",
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "latex_queue_depth",
            "legendFormat": "Queue Depth"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 50, "color": "yellow" },
                { "value": 80, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Active Jobs",
        "type": "stat",
        "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "latex_active_jobs",
            "legendFormat": "Active Jobs"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 5, "color": "yellow" },
                { "value": 10, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Total Compilations (5m rate)",
        "type": "stat",
        "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "rate(latex_compilation_total[5m]) * 60",
            "legendFormat": "Compilations/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2
          }
        }
      },
      {
        "id": 5,
        "title": "Compilation Duration (p50, p95, p99)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 4, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(latex_compilation_duration_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(latex_compilation_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(latex_compilation_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 6,
        "title": "Compilation Rate by Document Type",
        "type": "graph",
        "gridPos": { "x": 12, "y": 4, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "rate(latex_compilation_total{document_type=~\".*\"}[5m])",
            "legendFormat": "{{document_type}}"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Compilations/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 7,
        "title": "Error Rate by Type",
        "type": "graph",
        "gridPos": { "x": 0, "y": 10, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "rate(latex_compilation_error_total[5m])",
            "legendFormat": "{{error_type}}"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Errors/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 8,
        "title": "Package Cache Hit Rate",
        "type": "stat",
        "gridPos": { "x": 12, "y": 10, "w": 6, "h": 6 },
        "targets": [
          {
            "expr": "rate(latex_package_cache_hits_total[5m]) / (rate(latex_package_cache_hits_total[5m]) + rate(latex_package_cache_misses_total[5m])) * 100",
            "legendFormat": "Cache Hit Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 70, "color": "yellow" },
                { "value": 90, "color": "green" }
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": { "x": 18, "y": 10, "w": 6, "h": 6 },
        "targets": [
          {
            "expr": "latex_memory_usage_bytes",
            "legendFormat": "Heap Used"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "label": "Memory"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 10,
        "title": "Compilation Steps (Multi-pass)",
        "type": "graph",
        "gridPos": { "x": 0, "y": 16, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "rate(latex_compilation_steps_total[5m])",
            "legendFormat": "{{pass_type}}"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "label": "Steps/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 11,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": { "x": 12, "y": 16, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU %"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "CPU Usage",
            "max": 100
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 12,
        "title": "Error Distribution by Category",
        "type": "piechart",
        "gridPos": { "x": 0, "y": 22, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "sum by (error_category) (increase(latex_errors_by_type_total[1h]))",
            "legendFormat": "{{error_category}}"
          }
        ]
      },
      {
        "id": 13,
        "title": "Average Compilation Time by Document Type",
        "type": "table",
        "gridPos": { "x": 12, "y": 22, "w": 12, "h": 6 },
        "targets": [
          {
            "expr": "avg by (document_type) (rate(latex_compilation_duration_seconds_sum[5m]) / rate(latex_compilation_duration_seconds_count[5m]))",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "renameByName": {
                "document_type": "Document Type",
                "Value": "Avg Duration (s)"
              }
            }
          }
        ]
      }
    ],
    "refresh": "10s",
    "schemaVersion": 36,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
