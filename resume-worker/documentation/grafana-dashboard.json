{
  "dashboard": {
    "title": "Resume Worker Dashboard",
    "tags": ["resume-worker", "enginedge"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Total Evaluations (24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(resume_evaluations_total[24h]))",
            "legendFormat": "Evaluations"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Tailoring Jobs",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(bullmq_queue_size{queue=\"resume-tailoring\",state=\"active\"})",
            "legendFormat": "Active Jobs"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Experience Bank Size",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(experience_bank_items_total)",
            "legendFormat": "Total Items"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Success Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(resume_evaluations_total{status=\"success\"}[5m]) / rate(resume_evaluations_total[5m]) * 100",
            "legendFormat": "Success %"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "fieldConfig": {
          "defaults": {
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "P95 Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "Evaluation Scores",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(resume_evaluation_score)",
            "legendFormat": "Average Score"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "Bullet Pass Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bullet_evaluations_total{passed=\"true\"}[5m]) / rate(bullet_evaluations_total[5m]) * 100",
            "legendFormat": "Pass Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 9,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status_code=~\"5..\"}[5m])",
            "legendFormat": "5xx Errors"
          },
          {
            "expr": "rate(http_requests_total{status_code=~\"4..\"}[5m])",
            "legendFormat": "4xx Errors"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 10,
        "title": "Kafka Consumer Lag",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (topic) (kafka_consumer_lag)",
            "legendFormat": "{{topic}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 11,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "nodejs_heap_size_used_bytes / nodejs_heap_size_total_bytes * 100",
            "legendFormat": "Heap Usage %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100}
        ]
      },
      {
        "id": 12,
        "title": "Event Loop Lag",
        "type": "graph",
        "targets": [
          {
            "expr": "nodejs_eventloop_lag_seconds",
            "legendFormat": "Event Loop Lag"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28}
      },
      {
        "id": 13,
        "title": "BullMQ Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (queue, state) (bullmq_queue_size)",
            "legendFormat": "{{queue}} - {{state}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36}
      },
      {
        "id": 14,
        "title": "Job Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(bullmq_jobs_total{status=\"completed\"}[5m])",
            "legendFormat": "Completed"
          },
          {
            "expr": "rate(bullmq_jobs_total{status=\"failed\"}[5m])",
            "legendFormat": "Failed"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36}
      },
      {
        "id": 15,
        "title": "WebSocket Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (namespace) (websocket_connections_active)",
            "legendFormat": "{{namespace}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 44}
      },
      {
        "id": 16,
        "title": "MongoDB Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mongodb_operations_total[5m])",
            "legendFormat": "{{operation}} - {{collection}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 44}
      }
    ]
  }
}
